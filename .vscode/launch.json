  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Project",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/index.js",
      "runtimeExecutable": "/home/daniel/.asdf/shims/node",
      "args": [
        "project",
        "sebastianbergmann",
        "phpunit"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch File",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/index.js",
      "runtimeExecutable": "/home/daniel/.asdf/shims/node",
      "args": [
        "file",
        "projects"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Join Outputs",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/index.js",
      "runtimeExecutable": "/home/daniel/.asdf/shims/node",
      "args": [
        "join-outputs",
        "tmp/output.csv"
      ]
    }
  ]
}
